{
  "name": "kana",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "lint": "eslint . --ext mjs,js,ts --fix",
    "format": "prettier --write \"./{scripts,src}/**/*.{mjs,ts,js}\"",
    "build": "rimraf ./dist && swc ./src --out-dir ./dist --copy-files --strip-leading-paths",
    "dev": "wrangler dev ./dist/index.js",
    "register": "node ./scripts/register.js",
    "deploy": "wrangler deploy",
    "update": "yarn upgrade-interactive"
  },
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "@discordjs/formatters": "^0.5.0",
    "@discordjs/rest": "^2.4.0",
    "@rygent/anilist": "^1.5.0-next.f50a2bb.0",
    "@sapphire/time-utilities": "^1.7.12",
    "@sapphire/utilities": "^3.17.0",
    "discord-api-types": "^0.37.102",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.4.5",
    "globby": "^14.0.2",
    "he": "^1.2.0",
    "itty-router": "^4.2.2",
    "moment": "^2.30.1",
    "pino": "^8.21.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241011.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.35",
    "@types/he": "^1.2.3",
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.10",
    "typescript": "^5.6.3",
    "wrangler": "^3.80.4"
  },
  "packageManager": "yarn@4.5.0"
}
