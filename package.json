    "discord-interactions": "^3.4.0",
    "dotenv-cra": "^3.0.3",
    "glob": "^10.2.6",
    "globby": "^13.1.4",
    "he": "^1.2.0",
    "itty-router": "^3.0.12",
    "moment": "^2.29.4",
    "pino": "^8.14.1"
