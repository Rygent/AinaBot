    "@sapphire/utilities": "^3.13.0",
    "discord-api-types": "^0.37.63",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.3.1",
    "globby": "^14.0.0",
    "he": "^1.2.0",
    "itty-router": "^4.0.23",
    "moment": "^2.29.4",
    "pino": "^8.16.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@swc/cli": "^0.1.62",
