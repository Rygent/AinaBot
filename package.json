    "discord-interactions": "^3.4.0",
    "dotenv-cra": "^3.0.3",
    "glob": "^10.3.10",
    "globby": "^13.2.2",
    "he": "^1.2.0",
    "itty-router": "^4.0.23",
    "moment": "^2.29.4",
    "pino": "^8.16.1"
